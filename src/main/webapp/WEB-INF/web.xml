<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Web Application</display-name>
    <servlet>
   <servlet-name>Faces Servlet</servlet-name>
   <servlet-class>javax.faces.webapp.FacesServlet</servlet-class>
   <load-on-startup>1</load-on-startup>
  </servlet>
    <servlet>
    	<servlet-name>SendSupportEmail</servlet-name>
    	<display-name>SendSupportEmail</display-name>
    	<description></description>
    	<servlet-class>com.vfp.tres.SendSupportEmail</servlet-class>
    </servlet>
 
  <servlet-mapping>
   <servlet-name>Faces Servlet</servlet-name>
   <url-pattern>*.xhtml</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>SendSupportEmail</servlet-name>
  	<url-pattern>/SendSupportEmail</url-pattern>
  </servlet-mapping>
  
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>home.xhtml</welcome-file>
    </welcome-file-list>
    
	<error-page>
		<error-code>404</error-code>
		<location>/WEB-INF/404.xhtml</location>
	</error-page>
	
	<error-page>
		<error-code>500</error-code>
		<location>/WEB-INF/500.xhtml</location>
	</error-page>
	
	<error-page>
		<error-code>503</error-code>
		<location>/WEB-INF/503.xhtml</location>
	</error-page>
	
	<error-page>
		<error-code>403</error-code>
		<location>/WEB-INF/403.xhtml</location>
	</error-page>
	
</web-app>

